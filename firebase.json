{
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log"
      ]
    }
  ],
  "hosting": {
    "public": "app/web-build",
    "ignore": [
      "app/firebase.json",
      "app/**/.*",
      "app/**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "app/**",
        "destination": "app/index.html"
      }
    ]
  }
}